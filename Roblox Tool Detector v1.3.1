	local DefaultChatSystemChatEvents = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents")
	local SayMessageRequest = DefaultChatSystemChatEvents:WaitForChild("SayMessageRequest")

	local function countPlayerTools()
		local toolsFoundAnywhere = false

		for _, player in ipairs(Players:GetPlayers()) do
			local totalTools = 0

			if player.Backpack then
				for _, item in ipairs(player.Backpack:GetChildren()) do
					if item:IsA("Tool") then
						totalTools = totalTools + 1
					end
				end
			end

			if player.Character then
				for _, item in ipairs(player.Character:GetChildren()) do
					if item:IsA("Tool") then
						totalTools = totalTools + 1
					end
				end
			end

			if totalTools > 0 then
				toolsFoundAnywhere = true
				local message = "✔ | " .. player.DisplayName .. " has " .. totalTools
				if totalTools == 1 then
					message = message .. " tool"
				else
					message = message .. " tools"
				end
				SayMessageRequest:FireServer(message, "All")
			end
		end

		if not toolsFoundAnywhere then
			SayMessageRequest:FireServer("✖ | No tools were found.", "All")
		end
	end

	countPlayerTools()
end)

